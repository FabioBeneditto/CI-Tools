<phpunit
    colors="true"
    stopOnFailure="false"
    backupGlobals="false"
    backupStaticAttributes="false"
    convertErrorsToExceptions="true"
    convertNoticesToExceptions="true"
    convertWarningsToExceptions="true"
    processIsolation="false"
    syntaxCheck="false"
    cacheTokens="true"
    verbose="false"
    bootstrap="bootstrap.php">
    <testsuites>
        <testsuite name="CI-Code-Generator Test Suite">
            <directory>.</directory>
        </testsuite>
    </testsuites>
</phpunit>


